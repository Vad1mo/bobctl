{
	"name": "Go Development",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"GO_VERSION": "1.24"
		}
	},

	"settings": {
		"go.toolsManagement.checkForUpdates": "local",
		"go.useLanguageServer": true,
		"go.gopath": "/go",
		"go.goroot": "/usr/local/go"
	},

	"extensions": [
		"golang.go"
	],

	"forwardPorts": [],

	"remoteUser": "root"


}